{
  "name": "keep-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "node bin/www",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "lint": "standard src/**/*.js[x]",
    "lintfix": "standard --fix src/**/*.js[x]",
    "lint-staged": "LIST=`git diff-index --name-only HEAD | grep '\\.js[x]*$'`; if [ \"$LIST\" ]; then standard $LIST; fi",
    "lintfix-staged": "LIST=`git diff-index --name-only HEAD | grep '\\.js[x]*$'`; if [ \"$LIST\" ]; then standard --fix $LIST; fi"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "ghooks": "^2.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack-dev-server": "^2.9.3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lintfix-staged"
    }
  }
}
